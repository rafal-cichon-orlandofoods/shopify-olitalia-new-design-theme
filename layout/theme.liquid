        --font-stack-headings-primary: 'Soho Pro', Arial, sans-serif;
        --font-stack-body-primary: 'Raleway', Arial, sans-serif;
        --font-stack-body-secondary: 'Raleway', Arial, sans-serif;
        
        /* Set appropriate font weights */
        --font-weight-headings-primary: 500;
        --font-weight-body-primary: 400;
        --font-weight-body-primary-medium: 500;
        --font-weight-body-primary-bold: 700;
        --font-weight-body-secondary: 500;
        --font-weight-body-secondary-bold: 700;
      }
      
      /* Preserve original fonts in About Us page slider sections */
      .template-page-about #shopify-section-1662457455386507d1 .text-font--heading,
      .template-page-about #shopify-section-16625305896ba5775b .text-font--heading,
      .template-page-about #shopify-section-1662457455386507d1 h1,
      .template-page-about #shopify-section-1662457455386507d1 h2,
      .template-page-about #shopify-section-1662457455386507d1 h3,
      .template-page-about #shopify-section-16625305896ba5775b h1,
      .template-page-about #shopify-section-16625305896ba5775b h2,
      .template-page-about #shopify-section-16625305896ba5775b h3 {
        font-family: 'Instrument Serif', serif !important;
      }
      
      /* Product card video styling */
      .product-item__video-container {
        position: relative;
        overflow: hidden;
        cursor: pointer;
      }
      
      .product-item__video-container .product-item__video {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
        opacity: 0;
        transition: opacity 0.3s ease;
        pointer-events: none;
        z-index: 2;
      }
      
      .product-item__video-container:hover .product-item__video {
        opacity: 1;
        pointer-events: auto;
      }
      
      /* Ensure the image stays behind the video */
      .product-item__video-container .lazy-image {
        position: relative;
        z-index: 1;
      }
      
      /* Make submenu and mega menu whiter */
      .site-nav.style--classic .submenu:after,
      .site-nav.style--classic .submenu.normal-menu .babymenu,
      .site-nav.style--classic .submenu.mega-menu .submenu-holder {
        background: #ffffff !important;
      }
      
      /* Ensure submenu text is readable on white background */
      .site-nav.style--classic .submenu .menu-link {
        color: #000000;
      }
    </style>
